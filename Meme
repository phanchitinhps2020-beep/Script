    Desc = "More Than 2 Player To Use",
    Callback = function()
        -- L·∫•y t·∫•t c·∫£ c√°c RemoteEvent trong game
        local function triggerAllRemoteEvents()
            -- Duy·ªát qua t·∫•t c·∫£ c√°c ƒë·ªëi t∆∞·ª£ng trong game
            for _, object in pairs(game:GetDescendants()) do
                -- Ki·ªÉm tra xem ƒë·ªëi t∆∞·ª£ng c√≥ ph·∫£i l√† RemoteEvent hay kh√¥ng
                if object:IsA("RemoteEvent") then
                    -- K√≠ch ho·∫°t RemoteEvent
                    print("K√≠ch ho·∫°t RemoteEvent: " .. object.Name)
                    -- G·ª≠i t√≠n hi·ªáu ƒë·∫øn RemoteEvent (c√≥ th·ªÉ th√™m arguments n·∫øu c·∫ßn)
                    object:FireServer()
                end
            end
        end

        -- G·ªçi h√†m khi b·∫•m n√∫t
        triggerAllRemoteEvents()
    end
})

